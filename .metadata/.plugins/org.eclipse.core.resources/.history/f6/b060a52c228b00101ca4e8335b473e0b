public class Lab_05 {
	public static void main(String[] args) {
		Locale.setDefault(Locale.US);

		Scanner scanner = new Scanner(System.in);

		int tuoi = 0;
		do {
			System.out.print("Nhap vao mot so: ");
			int tuoi1 = scanner.nextInt();
		} while (tuoi >= 0 && tuoi <= 10);

	}
}
